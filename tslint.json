{
    "defaultSeverity": "error",
    "no-default-export": false,
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single",
            "avoid-escape",
            "jsx-double"
        ],
        "trailing-comma": false,
        "no-var-keyword": true,
        "indent": [
            true,
            "spaces",
            2
        ],
        "eofline": false,
        "whitespace": false,
        "import-spacing": false,
        "prefer-const": true,
        "member-access": false,
        "no-unused-expression": false
    },
    "rulesDirectory": []
}
